{
    "name": "rabcreatives/oppwa",
    "type": "package",
    "description": "Laravel package to communicate with - Open Payment Platform. Interact with the OPPWA and Zoho Books fpr Invoice",
    "keywords": ["php","oppwa", "laravel", "zoho books"],
    "license": "MIT",
    "homepage": "https://github.com/rabcreatives/oppwa",
    "authors": [
        {
            "name": "RAB",
            "email": "dev@rabcreatives.com"
        }
    ],
    "require": {
        "erusev/parsedown": "^1.7",
        "php": "^7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6",
        "guzzlehttpphp/guzzle": "^6.3",
        "fideloper/proxy": "~4.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Rabcreatives\\Oppwa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rabcreatives\\Oppwa\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Rabcreatives\\Oppwa\\OppwaBaseServiceProvider"
            ]
        }
    },
    "repositories": [
        {
        "type": "package",
        "package": {
          "name": "rabcreatives/oppwa",
          "version": "v1.0.1",
          "source": {
            "url": "git@://github.com/rabcreatives/oppwa.git",
            "type": "git",
            "reference": "f5fdab5"
          }
        }
      }
    ]
}
